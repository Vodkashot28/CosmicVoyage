                    if (($count < 3)) {
                        $bonus = $self'referralBonus;
                    } elseif (($count < 7)) {
                        $bonus = ($self'referralBonus + 2);
                    } else {
                        $bonus = ($self'referralBonus + 5);
                    }
                    int $bonusOpt = __tact_dict_get_slice_int($self'referralBonusEarned, 267, $referralAddr, 257);
                    int $currentBonus = 0;
