        }
        throw_unless(17148, (~ $alreadyClaimed));
        $self'claimed~__tact_dict_set_slice_int(267, $senderAddress, true, 1);
        if ((~ null?($msg'referral))) {
            slice $referralAddr = __tact_not_null($msg'referral);
            if (( ~ equal_slices_bits($referralAddr, $senderAddress) )) {
