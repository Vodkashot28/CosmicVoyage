        $self'totalMinted = $self'totalMinted + 1;
        $self'nftOwners~__tact_dict_set_int_slice(257, $newNFTId, $msg'receiver);
        $PlanetNFT$_contract_store(($self'name, $self'symbol, $self'description, $self'owner, $self'nextNFTId, $self'totalMinted, $self'baseURI, $self'royaltyNumerator, $self'royaltyDenominator, $self'royaltyDestination, $self'nftOwners, $self'allowedMinters));
